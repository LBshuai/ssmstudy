<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.mapper.BanjiMapper">
		<resultMap type="Banji" id="banjiResult">
			<id property="id" column="id"/>
			<result property="name" column="name"/>
		</resultMap>
		<insert id="insert" parameterType="Banji">
			insert into banji (name) values (#{name})
		</insert>
		<update id="update" parameterType="Banji">
			update banji set name=#{name} where id=#{id}
		</update>
		<delete id="delete" parameterType="Banji">
			delete from banji where id= #{id}
		</delete>
		<select id="list" resultType="Banji" resultMap="banjiResult">
			select * from  banji where 1=1 ${where} order by id desc
			</select>
		<select id="sealist" resultType="Banji" resultMap="banjiResult">
			select * from  banji where 1=1 ${where} 
			</select>
		<select id="get" parameterType="_int" resultType="Banji">
			select * from  Banji  where id= #{id}
		</select>
			<select id="total" resultType="int">
	        select count(*) from banji    
	    </select>
<update id="updatesql"  parameterType="String">
${value}
</update>
<delete id="deletesql"  parameterType="String">
${value}  
</delete>
	</mapper>