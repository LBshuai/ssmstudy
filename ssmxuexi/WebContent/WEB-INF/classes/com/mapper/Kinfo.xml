<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.mapper.KinfoMapper">
		<resultMap type="Kinfo" id="kinfoResult">
			<id property="id" column="id"/>
			<result property="name" column="name"/>
		</resultMap>
		<insert id="insert" parameterType="Kinfo">
			insert into kinfo (name) values (#{name})
		</insert>
		<update id="update" parameterType="Kinfo">
			update kinfo set name=#{name} where id=#{id}
		</update>
		<delete id="delete" parameterType="Kinfo">
			delete from kinfo where id= #{id}
		</delete>
		<select id="list" resultType="Kinfo" resultMap="kinfoResult">
			select * from  kinfo where 1=1 ${where} order by id desc
			</select>
		<select id="sealist" resultType="Kinfo" resultMap="kinfoResult">
			select * from  kinfo where 1=1 ${where} 
			</select>
		<select id="get" parameterType="_int" resultType="Kinfo">
			select * from  Kinfo  where id= #{id}
		</select>
			<select id="total" resultType="int">
	        select count(*) from kinfo    
	    </select>
<update id="updatesql"  parameterType="String">
${value}
</update>
<delete id="deletesql"  parameterType="String">
${value}  
</delete>
	</mapper>